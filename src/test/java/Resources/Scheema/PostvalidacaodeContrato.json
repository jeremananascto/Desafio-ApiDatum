{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "name",
    "job",
    "id",
    "createdAt"
  ],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "joao"
      ]
    },
    "job": {
      "type": "string",
      "default": "",
      "title": "The job Schema",
      "examples": [
        "service"
      ]
    },
    "id": {
      "type": "string",
      "default": "",
      "title": "The id Schema",
      "examples": [
        "997"
      ]
    },
    "createdAt": {
      "type": "string",
      "default": "",
      "title": "The createdAt Schema",
      "examples": [
        "2022-07-09T19:51:02.199Z"
      ]
    }
  },
  "examples": [{
    "name": "joao",
    "job": "service",
    "id": "997",
    "createdAt": "2022-07-09T19:51:02.199Z"
  }]
}